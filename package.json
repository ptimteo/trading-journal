{
  "name": "trading-journal",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "start": "vite --host",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "stop": "taskkill /F /IM node.exe",
    "server": "nodemon server/app.js",
    "backend-api": "cd ../backend && node index.js",
    "dev:full": "concurrently \"npm run dev\" \"npm run server\" \"npm run backend-api\"",
    "test:mongo": "node test-mongodb-connection.js",
    "test:db": "node test-db-connection.js",
    "check:mongo": "node setup-mongo.js",
    "start:safe": "node setup-mongo.js && npm run dev:full"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.23",
    "@types/uuid": "^10.0.0",
    "@vueuse/core": "^10.11.1",
    "axios": "^1.8.4",
    "bcryptjs": "^2.4.3",
    "body-parser": "^2.2.0",
    "chart.js": "^4.4.8",
    "chartjs-adapter-date-fns": "^3.0.0",
    "coinmarketcap-api": "^3.1.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "element-plus": "^2.5.3",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.1.5",
    "finnhub": "^1.2.19",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.15.0",
    "mongoose": "^8.2.3",
    "node-html-parser": "^7.0.1",
    "pinia": "^2.1.7",
    "uuid": "^11.1.0",
    "vue": "^3.5.13",
    "vue-chartjs": "^5.3.2",
    "vue-router": "^4.2.5",
    "xss-clean": "^0.1.4",
    "yahoo-finance2": "^2.13.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.16",
    "@vitejs/plugin-vue": "^5.2.3",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.4.14",
    "vue-tsc": "^1.8.27"
